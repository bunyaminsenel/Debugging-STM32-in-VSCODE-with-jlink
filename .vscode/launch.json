    "version": "0.2.0",
    "configurations": [        
        {
            "name": "Test01",
            "cwd": "${workspaceRoot}",
            "executable": "./build/Test01.elf",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "jlink",
            "serverpath": "C:/Program Files/SEGGER/JLink/JLinkGDBServerCL.exe",
            "armToolchainPath": "${workspaceRoot}/tools/gcc-arm-none-eabi-10.3-2021.10/bin",
            "device": "STM32L433RC",
            "interface": "swd",
            "runToMain": true,
            "svdFile": "${workspaceRoot}/tools/STM32L4x3.svd",
            "swoConfig":{
                "enabled": true,
                "cpuFrequency": 80000000,
                "swoFrequency": 4000000,
                "source": "probe",
                "decoders": [
                    {
                        "label": "ITM port 0 output",
                        "type": "console",
                        "port": 0,
                        "showOnStartup": true,
                        "encoding": "ascii"
                        
                    }
                ]

            }

        }
    ]
}